<div class="list-group list-group-flush">
    {% for blog in blogs %}
            <a href="{{path('blog', {'blogSlug': blog.slug})}}" class="list-group-item list-group-item-action d-flex justify-content-between align-items-center">
                {{blog.title}}
                <span class="badge badge-primary badge-pill">{{blog.count}}</span>
            </a>
    {% endfor %}
</div>